<Project>
    <Target Name="ANcpLuaRoslynUtilitiesGenerateThisAssemblyVersion" BeforeTargets="CoreCompile">
        <PropertyGroup>
            <_ANcpLuaRoslynUtilitiesThisAssemblyVersionContent><![CDATA[// <auto-generated/>
namespace $(ANcpLuaRoslynUtilitiesNamespace)%3B

internal static partial class ThisAssembly
{
    public const string Version = "$(Version)"%3B
}
]]></_ANcpLuaRoslynUtilitiesThisAssemblyVersionContent>
        </PropertyGroup>
        <WriteLinesToFile File="$(IntermediateOutputPath)ANcpLua.Roslyn.Utilities.ThisAssembly.g.cs"
                          Lines="$(_ANcpLuaRoslynUtilitiesThisAssemblyVersionContent)"
                          Overwrite="true"
                          WriteOnlyWhenDifferent="true"/>
        <ItemGroup>
            <Compile Include="$(IntermediateOutputPath)ANcpLua.Roslyn.Utilities.ThisAssembly.g.cs" Visible="false"/>
        </ItemGroup>
    </Target>
</Project>
